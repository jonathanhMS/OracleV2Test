{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "jonathanhADFOracle2-Dev"
		},
		"Oracle1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Oracle1'"
		},
		"OracleV1_0_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OracleV1_0'"
		},
		"Oracle1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "oracle.com"
		},
		"Oracle1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Test"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "jonathanhADFOracle2-Dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Oracle1_password": {
						"value": "[parameters('Oracle1_password')]"
					},
					"OracleV1_0_connectionString": {
						"value": "[parameters('OracleV1_0_connectionString')]"
					},
					"Oracle1_properties_typeProperties_server": {
						"value": "[parameters('Oracle1_properties_typeProperties_server')]"
					},
					"Oracle1_properties_typeProperties_username": {
						"value": "[parameters('Oracle1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}